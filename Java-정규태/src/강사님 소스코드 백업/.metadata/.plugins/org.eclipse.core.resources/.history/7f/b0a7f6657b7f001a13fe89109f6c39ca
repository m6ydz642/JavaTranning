
public class Test1 {

	public static void main(String[] args) {
		// 2차원 배열 (다차원 배열) : 행렬표시 가능(표)
		// => 반복문의 개수 증가 : 이중for문
		// 1. new 연산자
		// 2. 직접 초기화 
		
		// 5 X 3 행렬을 표현하기위한 2차원 배열 생성(정수형)
		//int[][] arr = new int[5][3];
		int[] arr[] = new int[5][3];
		
		arr[0][0] = 10; arr[0][1] = 90; arr[0][2] = 70; // 0행
		arr[1][0] = 60; arr[1][1] = 80; arr[1][2] = 65; // 1행
		arr[2][0] = 55; arr[2][1] = 60; arr[2][2] = 85; // 2행
		arr[3][0] = 90; arr[3][1] = 75; arr[3][2] = 95; // 3행
		arr[4][0] = 60; arr[4][1] = 30; arr[4][2] = 80; // 4행
		
		// 2차원 배열의 모양 그대로 출력
		
		
		
		
		
		
		
		

	}

}
