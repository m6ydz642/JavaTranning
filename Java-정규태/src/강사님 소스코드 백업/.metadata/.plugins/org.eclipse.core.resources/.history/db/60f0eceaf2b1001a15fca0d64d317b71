class GuGu implements Runnable{
	int dan;
	
	public GuGu() {	}
	public GuGu(int dan) {
		this.dan = dan;
	}	

	@Override
	public void run() {
		// 구구단 출력 
		
	}
	
}


public class Test2 {

	public static void main(String[] args) {
		
		// 구구단을 출력 (세로)
		// 2~9단 (랜덤한 순서로 출력) ex)2...9....4...7...6..8...3
		// => 구구단 객체를 생성해서 사용
		
		
		

	}

}
